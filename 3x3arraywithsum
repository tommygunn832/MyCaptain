#include <stdio.h>

void main()
{
  int arr1[3][3],i,j;
  int sum=0;
 
    printf("\n\nRead a 2D array of size 3x3 and print the matrix :\n");
    printf("Input elements in the matrix :\n");
  for(i=0;i<3;i++)
  {
      for(j=0;j<3;j++)
      {
	      printf("element - [%d],[%d] : ",i,j);
	      scanf("%d",&arr1[i][j]);
      }
  }  
 
 printf("\nThe matrix is : \n");
  for(i=0;i<3;i++)
  {
      printf("\n");
      for(j=0;j<3;j++)
           printf("%d\t",arr1[i][j]);
  }
 printf("\n\n");
 for(j = 0; j < i; j++)
  	{
   		sum = sum + arr1[i][j];
  	}
 	printf("\n The Sum of Diagonal Elements of a Matrix =  %d", sum );
 	
}
