#include<stdio.h> 
int main() 
{ 
    int marks;
    printf("Enter marks: ");
    scanf("%d",&marks);
    if(marks >= 85){
        printf("Grade A");
    }
    else if(marks <= 84 && marks >= 70){
        printf("Grade B");
    }
    else if(marks <= 69 && marks >= 55){
        printf("Grade C");
    }
    else if(marks <= 40 && marks >= 54){
        printf("Grade D");
    }
    else{
        printf("Grade F");
    }
}
